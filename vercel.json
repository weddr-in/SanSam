{
    "rewrites": [
        {
            "source": "/api/spotify-token",
            "destination": "https://accounts.spotify.com/api/token"
        },
        {
            "source": "/api/spotify/(.*)",
            "destination": "https://api.spotify.com/v1/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}